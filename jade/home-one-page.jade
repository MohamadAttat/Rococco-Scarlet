block page_header
	include includes/_header-one-page
block page_title_slider
	.top-header.top-bg-parallax
		.slides.parallax-window(data-parallax="scroll", data-image-src="assets/images/slider/slider2-bg1.jpg")
			.slide-content.slide-layout-02
				.container
					.slide-content-inner
						img.slide-icon.img.img-fluid.animated(src='assets/images/slider/slider2-icon.png', data-ani-in="fadeInUp", data-ani-out="fadeOutDown", data-ani-delay="500",alt="fooday")
						h3.slide-title.animated(data-ani-in="fadeInUp", data-ani-out="fadeOutDown", data-ani-delay="1000") FOODAY RESTAURANT
						p.slide-sub-title.animated( data-ani-in="fadeInUp", data-ani-out="fadeOutDown", data-ani-delay="1500")
							span.line-before
							span.line-after
							span.text
								span Tasty
								span Delicious
								span Savoury
block append config
	- var page_title = 'Home'
	- var Current_page = 'Home' 
	- var Current_parent = "Home"
