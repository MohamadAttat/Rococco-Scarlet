				.container
					.slide-content-inner.text-center
						img.d-block.img.img-fluid.slide-icon.animated(src='assets/images/slider/slider3-icon.png', data-ani-in="fadeInUp", data-ani-out="fadeOutDown", data-ani-delay="500",alt="fooday")
