block page_header
	include includes/_header-no-top
block page_title_slider
	.top-header.top-bg-video
		.slides(data-vide-bg="mp4: assets/video/fooday.mp4, webm: assets/video/fooday.webm, ogv: assets/video/fooday.ogv, poster: assets/video/fooday.jpg" data-vide-options="loop: true, muted: true, position: 0% 0%, autoplay: false")
